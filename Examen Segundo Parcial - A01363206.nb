Cell[CellGroupData[{

Cell["M\[OAcute]dulo de Secante", "Subsection",
 CellChangeTimes->{{3.537566031026399*^9, 3.5375660365737166`*^9}, {
  3.6128762052645736`*^9, 3.612876207706713*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Secante", "[", 
   RowBox[{"x0_", ",", "x1_", ",", "max_"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"k", " ", "=", " ", "1"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"p0", " ", "=", " ", 
      RowBox[{"N", "[", "x0", "]"}]}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"p1", " ", "=", " ", 
      RowBox[{"N", "[", "x1", "]"}]}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<f[x] = \>\"", ",", 
       RowBox[{"f", "[", "x", "]"}]}], " ", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{
     "Print", "[", 
      "\"\<\!\(\*SubscriptBox[\(p\), \(k + 1\)]\) = \
g[\!\(\*SubscriptBox[\(p\), \(k\)]\),\!\(\*SubscriptBox[\(p\), \(k - 1\)]\)] \
= \!\(\*SubscriptBox[\(p\), \(k\)]\) - \
\!\(\*FractionBox[\(f[\*SubscriptBox[\(p\), \(k\)]] \((\*SubscriptBox[\(p\), \
\(k\)] - \*SubscriptBox[\(p\), \(k - 1\)])\)\), \(f[\*SubscriptBox[\(p\), \(k\
\)]] - f[\*SubscriptBox[\(p\), \(k - 1\)]]\)]\)\>\"", "]"}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<\!\(\*SubscriptBox[\(p\), \(0\)]\) = \>\"", ",", 
       RowBox[{"PaddedForm", "[", 
        RowBox[{"p0", ",", 
         RowBox[{"{", 
          RowBox[{"16", ",", "16"}], "}"}]}], "]"}], ",", 
       "\"\<,   f[\!\(\*SubscriptBox[\(p\), \(0\)]\)] = \>\"", ",", 
       RowBox[{"NumberForm", "[", 
        RowBox[{
         RowBox[{"f", "[", "p0", "]"}], ",", "16"}], "]"}]}], " ", "]"}], ";",
      " ", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<\!\(\*SubscriptBox[\(p\), \(1\)]\) = \>\"", ",", 
       RowBox[{"PaddedForm", "[", 
        RowBox[{"p1", ",", 
         RowBox[{"{", 
          RowBox[{"16", ",", "16"}], "}"}]}], "]"}], ",", 
       "\"\<,   f[\!\(\*SubscriptBox[\(p\), \(1\)]\)] = \>\"", ",", 
       RowBox[{"NumberForm", "[", 
        RowBox[{
         RowBox[{"f", "[", "p1", "]"}], ",", "16"}], "]"}]}], " ", "]"}], ";",
      "  ", "\[IndentingNewLine]", 
     RowBox[{"p2", " ", "=", " ", "p1"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"p1", " ", "=", " ", "p0"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"While", "[", " ", 
      RowBox[{
       RowBox[{"k", "<", "max"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"p0", " ", "=", " ", "p1"}], ";", " ", "\[IndentingNewLine]", 
        RowBox[{"p1", " ", "=", " ", "p2"}], ";", " ", "\[IndentingNewLine]", 
        RowBox[{"p2", " ", "=", " ", 
         RowBox[{"p1", " ", "-", " ", 
          FractionBox[
           RowBox[{
            RowBox[{"f", "[", "p1", "]"}], 
            RowBox[{"(", 
             RowBox[{"p1", " ", "-", " ", "p0"}], ")"}]}], 
           RowBox[{
            RowBox[{"f", "[", "p1", "]"}], " ", "-", " ", 
            RowBox[{"f", "[", "p0", "]"}]}]]}]}], ";", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"k", " ", "=", " ", 
         RowBox[{"k", "+", "1"}]}], ";", " ", "\[IndentingNewLine]", 
        RowBox[{"Print", "[", 
         RowBox[{
          SubscriptBox["\"\<p\>\"", "k"], ",", "\"\< = \>\"", ",", 
          RowBox[{"PaddedForm", "[", 
           RowBox[{"p2", ",", 
            RowBox[{"{", 
             RowBox[{"16", ",", "16"}], "}"}]}], "]"}], ",", "\"\<,   f[\>\"",
           ",", 
          SubscriptBox["\"\<p\>\"", "k"], ",", "\"\<] = \>\"", ",", 
          RowBox[{"NumberForm", "[", 
           RowBox[{
            RowBox[{"f", "[", "p2", "]"}], ",", "16"}], "]"}]}], " ", "]"}], 
        ";"}]}], " ", "]"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", "\"\<\>\"", "]"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<f[x] = \>\"", ",", 
       RowBox[{"f", "[", "x", "]"}]}], " ", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<  p  = \>\"", ",", 
       RowBox[{"NumberForm", "[", 
        RowBox[{"p2", ",", "16"}], "]"}]}], " ", "]"}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\< \[CapitalDelta]p  = \[PlusMinus]\>\"", ",", 
       RowBox[{"Abs", "[", 
        RowBox[{"p2", "-", "p1"}], "]"}]}], " ", "]"}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<f[p] = \>\"", ",", 
       RowBox[{"NumberForm", "[", 
        RowBox[{
         RowBox[{"f", "[", "p2", "]"}], ",", "16"}], "]"}]}], " ", "]"}], 
     ";"}]}], " ", "]"}], " "}]], "Input",
 CellChangeTimes->{{3.537566050428509*^9, 3.5375660525716314`*^9}}]
}, Open  ]],

Cell[CellGroupData[{

Cell["M\[OAcute]dulo de Newton Raphson", "Subsection",
 CellChangeTimes->{{3.537568144134262*^9, 3.5375681470464287`*^9}, {
  3.612876222524561*^9, 3.612876226860809*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"NewtonRaphson", "[", 
   RowBox[{"x0_", ",", "max_"}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"k", " ", "=", " ", "0"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"p0", " ", "=", " ", 
      RowBox[{"N", "[", "x0", "]"}]}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<f[x] = \>\"", ",", 
       RowBox[{"f", "[", "x", "]"}]}], " ", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"g", "[", "x_", "]"}], "=", 
      RowBox[{"x", "-", 
       FractionBox[
        RowBox[{"f", "[", "x", "]"}], 
        RowBox[{
         RowBox[{"f", "'"}], "[", "x", "]"}]]}]}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{
     "Print", "[", 
      "\"\<g[x] = x - \!\(\*FractionBox[\(f[x]\), \(\(f'\)[x]\)]\)\>\"", 
      "]"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<g[x] = \>\"", ",", 
       RowBox[{"g", "[", "x", "]"}]}], " ", "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<g[x] = \>\"", ",", 
       RowBox[{"Simplify", "[", 
        RowBox[{"g", "[", "x", "]"}], "]"}]}], "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<  \!\(\*SubscriptBox[\(p\), \(0\)]\) = \>\"", ",", 
       RowBox[{"PaddedForm", "[", 
        RowBox[{"p0", ",", 
         RowBox[{"{", 
          RowBox[{"16", ",", "16"}], "}"}]}], "]"}], ",", 
