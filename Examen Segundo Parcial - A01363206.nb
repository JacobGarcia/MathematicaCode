Cell[CellGroupData[{

Cell["M\[OAcute]dulo de Bisecci\[OAcute]n", "Subsection",
 CellChangeTimes->{{3.5375647499231243`*^9, 3.5375647546293936`*^9}, {
  3.6128761735537596`*^9, 3.6128761767229414`*^9}}],

Cell[BoxData[
 RowBox[{
  RowBox[{"Biseccion", "[", 
   RowBox[{"a0_", ",", "b0_", ",", "m_"}], "]"}], " ", ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"a", " ", "=", " ", 
      RowBox[{"N", "[", "a0", "]"}]}], ";", "  ", "\[IndentingNewLine]", 
     RowBox[{"b", " ", "=", " ", 
      RowBox[{"N", "[", "b0", "]"}]}], ";", "  ", "\[IndentingNewLine]", 
     RowBox[{"c", " ", "=", " ", 
      FractionBox[
       RowBox[{"a", " ", "+", " ", "b"}], "2"]}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"k", " ", "=", " ", "0"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"output", "=", 
      RowBox[{"{", 
       RowBox[{"{", 
        RowBox[{"k", ",", "a", ",", "c", ",", "b", ",", 
         RowBox[{"f", "[", "c", "]"}]}], "}"}], "}"}]}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{
       RowBox[{"k", " ", "<", " ", "m"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"If", "[", " ", 
         RowBox[{
          RowBox[{
           RowBox[{"Sign", "[", 
            RowBox[{"f", "[", "b", "]"}], "]"}], " ", "\[Equal]", "  ", 
           RowBox[{"Sign", "[", 
            RowBox[{"f", "[", "c", "]"}], "]"}]}], ",", " ", 
          "\[IndentingNewLine]", 
          RowBox[{"b", " ", "=", " ", "c"}], ",", " ", 
          RowBox[{
           RowBox[{"a", " ", "=", " ", "c"}], ";"}]}], " ", "]"}], ";", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"c", " ", "=", " ", 
         FractionBox[
          RowBox[{"a", " ", "+", " ", "b"}], "2"]}], ";", " ", 
        "\[IndentingNewLine]", 
        RowBox[{"k", " ", "=", " ", 
         RowBox[{"k", "+", "1"}]}], ";", " ", "\[IndentingNewLine]", 
        RowBox[{"output", "=", 
         RowBox[{"Append", "[", 
          RowBox[{"output", ",", 
           RowBox[{"{", 
            RowBox[{"k", ",", "a", ",", "c", ",", "b", ",", 
             RowBox[{"f", "[", "c", "]"}]}], "}"}]}], "]"}]}], ";"}]}], " ", 
      "]"}], ";", " ", "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"NumberForm", "[", 
       RowBox[{
        RowBox[{"TableForm", "[", 
         RowBox[{"output", ",", "\[IndentingNewLine]", 
          RowBox[{"TableHeadings", "\[Rule]", 
           RowBox[{"{", 
            RowBox[{"None", ",", 
             RowBox[{"{", 
              RowBox[{
              "\"\<k\>\"", ",", "\"\<\!\(\*SubscriptBox[\(a\), \(k\)]\)\>\"", 
               ",", "\"\<\!\(\*SubscriptBox[\(c\), \(k\)]\)\>\"", ",", 
               "\"\<\!\(\*SubscriptBox[\(b\), \(k\)]\)\>\"", ",", 
               "\"\<f[\!\(\*SubscriptBox[\(c\), \(k\)]\)]\>\""}], "}"}]}], 
            "}"}]}]}], "]"}], ",", "16"}], "]"}], "]"}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<  c  = \>\"", ",", 
       RowBox[{"NumberForm", "[", 
        RowBox[{"c", ",", "16"}], "]"}]}], " ", "]"}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\< \[CapitalDelta]c  = \[PlusMinus]\>\"", ",", 
       FractionBox[
        RowBox[{"b", "-", "a"}], "2"]}], "]"}], ";", " ", 
     "\[IndentingNewLine]", 
     RowBox[{"Print", "[", 
      RowBox[{"\"\<f[c] = \>\"", ",", 
       RowBox[{"NumberForm", "[", 
        RowBox[{
         RowBox[{"f", "[", "c", "]"}], ",", "16"}], "]"}]}], " ", "]"}], 
     ";"}]}], " ", "]"}], "  "}]], "Input",
 CellChangeTimes->{{3.5375647649399834`*^9, 3.5375647685201883`*^9}}]
